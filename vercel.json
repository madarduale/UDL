{
    "builds": [
        {
            "src": "install_weasyprint.sh",
            "use": "@vercel/static-build"
        },
        {
            "src": "UDL_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "UDL_project/wsgi.py"
        }
    ]
}
